#!/bin/bash
#SBATCH --job-name="lmtp_tmle"
#SBATCH -e lmtp_tmle.err
#SBATCH -p mpi
#SBATCH -n 100
#SBATCH --ntasks-per-node=10
#SBATCH --mem-per-cpu=25GB
#SBATCH -t 1-00:00:00
#SBATCH --array=1-3

ulimit -l unlimited

module load gcc/6.2.0 R/4.0.1
module load openmpi/4.1.1

#Run the job
mpirun --mca btl tcp,self Rscript simulation.R 'lmtp_tmle' ${SLURM_ARRAY_TASK_ID} 'TRUE' 'TRUE'